{
  "name": "codelingual",
  "version": "0.1.0",
  "description": "The purpose of this web application is to help individuals quickly learn the basics of new languages by translating standard and common functions from one language to another (so like Google Translate, but for code)!",
  "main": "./client/public/index.html",
  "scripts": {
    "server": "(cd server && npm run server)",
    "win-install-all": "(cd client && npm install) && (cd ../server && npm install)",
    "lin-install-all": "(cd client && npm install); (cd ../server && npm install)"
  },
